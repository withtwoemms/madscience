<% provide :title, "Log in" %>
<h1>Log in</h1>

<div class="">
  <div class="">
    <%= form_for :session, url: login_path do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in" %>
    <% end %>    
  </div>
</div>